{
  "name": "hedera-mirrornode-ts",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "default": "./src/types/index.ts"
    },
    "./openapi": {
      "types": null,
      "default": null
    }
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "scripts": {
    "package": "rslib build",
    "t:package": "rslib build",
    "build": "node ./script/build.js",
    "gen:openapi-types": "openapi --input ./src/openapi/openapi.yml --output ./src/openapi/types --useUnionTypes --exportCore false --exportServices false",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "format:git": "biome format --no-errors-on-unmatched --files-ignore-unknown=true  --staged  *.{js,ts,cjs,mjs,d.ts,d.cts,d.mts,jsx,tsx,json,jsonc} --write",
    "test": "vitest run",
    "test:dev": "vitest dev ",
    "rslib": "rslib",
    "test:browser": "vitest --config=vitest.browser.config.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@rslib/core": "^0.17.1",
    "@types/node": "^24.10.0",
    "@vitest/browser-playwright": "^4.0.8",
    "@vitest/ui": "4.0.8",
    "lefthook": "^2.0.3",
    "openapi-typescript-codegen": "^0.29.0",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8",
    "@hey-api/openapi-ts": "^0.87.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "core-js",
      "esbuild",
      "lefthook"
    ]
  }
}
